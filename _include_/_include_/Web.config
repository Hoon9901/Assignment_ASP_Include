<configuration>
  <connectionStrings>
    <add name="db_userConnectionString1" connectionString="Data Source=(local)\sqlexpress;Initial Catalog=db_user;Integrated Security=True;Pooling=False"
      providerName="System.Data.SqlClient" />
    <add name="mycon" connectionString="server=(local)\SQLExpress;Integrated Security=true;database=db_user" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1"/>
  </system.web>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="WebForms" />
  </appSettings>
</configuration>
